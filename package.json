        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-preset-env": "^1.4.0",
    "bili": "^0.14.3",
    "documentation": "^4.0.0-rc.1",
    "eslint-config-rem": "^3.0.0",
    "jest-cli": "^19.0.0",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem/prettier",
    "esnext": true,
    "envs": [
      "jest"
    ],
    "rules": {
      "import/extensions": 0
    }
  }
